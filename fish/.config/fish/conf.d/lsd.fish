if status is-interactive
	abbr -a l   lsd
	abbr -a la  lsd -a
	abbr -a ls  lsd
	abbr -a ll  lsd -l
	abbr -a lt  lsd --tree -l
	abbr -a lt2 lsd --tree -l --depth 2
	abbr -a lt3 lsd --tree -l --depth 3
	abbr -a ltd lsd --tree -l --depth
	abbr -a t   lsd --tree
	abbr -a t2  lsd --tree --depth 2
	abbr -a t3  lsd --tree --depth 3
	abbr -a td  lsd --tree --depth
end
